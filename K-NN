{"cells":[{"cell_type":"code","source":["import numpy as np\n","import matplotlib.pyplot as plt\n","from sklearn.model_selection import train_test_split\n","from sklearn.neighbors import KNeighborsClassifier\n","from sklearn.metrics import accuracy_score\n","\n","# Gerando um conjunto de dados de exemplo\n","np.random.seed(0)\n","X = np.random.rand(100, 2)  # 100 amostras, 2 características\n","y = np.where(X[:, 0] + X[:, 1] > 1, 1, 0)  # Classe 1 se a soma das características for maior que 1, caso contrário, classe 0\n","\n","# Dividindo o conjunto de dados em treinamento e teste\n","X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.2, random_state=0)\n","\n","# Criando o classificador k-NN\n","k = 3  # Número de vizinhos\n","knn = KNeighborsClassifier(n_neighbors=k)\n","\n","# Treinando o classificador\n","knn.fit(X_train, y_train)\n","\n","# Fazendo previsões no conjunto de teste\n","y_pred = knn.predict(X_test)\n","\n","# Avaliando a precisão do classificador\n","accuracy = accuracy_score(y_test, y_pred)\n","print(f'Acurácia: {accuracy * 100:.2f}%')\n","\n","# Plotando os dados de treinamento\n","plt.scatter(X_train[:, 0], X_train[:, 1], c=y_train, cmap='coolwarm', edgecolor='k', s=20, label='Treinamento')\n","\n","# Plotando os dados de teste\n","plt.scatter(X_test[:, 0], X_test[:, 1], c=y_pred, cmap='coolwarm', edgecolor='k', marker='x', s=50, label='Teste')\n","\n","# Adicionando título e legendas\n","plt.title('Classificação k-NN')\n","plt.xlabel('Característica 1')\n","plt.ylabel('Característica 2')\n","plt.legend()\n","plt.show()\n"],"metadata":{"id":"zZp14IremaPZ","executionInfo":{"status":"ok","timestamp":1724775510194,"user_tz":180,"elapsed":1480,"user":{"displayName":"","userId":""}},"outputId":"cea685ad-6f44-420c-acb2-85d5217a3ea1","colab":{"base_uri":"https://localhost:8080/","height":544}},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Acurácia: 90.00%\n"]},{"output_type":"stream","name":"stder